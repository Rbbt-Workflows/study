Study=/$PWD filter="*.rb *.R *.js *.sass *.haml *.tsv" {
 workflow.rb
 workflow.md
 lib=lib {
  sinatra.rb

  study.rb
  tasks=study/tasks{
   samples.rb
   genotypes.rb
   cnv.rb
   expression.rb
   pancancer.rb
   maf.rb

   snv.rb
   snv=snv{
    cohort.rb
   }
  }

  entity=rbbt/entity{
   study.rb
   study=study{
    genotypes.rb
    expression.rb
    cnv.rb
    snp.rb
    methylation.rb
   }
  }
 }
 etc=etc{
  persisted_properties
  study_repo
 }
 share=share{
  rbbt_commands=rbbt_commands filter="*"{
   bootstrap
   knowledge_base
   maf2study
  }
 }
 www=www{
  views=views{
   entity=entity{
    Study.haml
    Study=Study{
     binomial_significance.haml
     MutSig.haml
     count_tables.haml
     gene_list_report.haml
     gene_report.haml
     mutation_list_report.haml
     mutation_report.haml
     gene_damage_bias.haml
    }
    Gene=Gene{
     study_report.haml
     incidence.haml
    }
    GenomicMutation=GenomicMutation{
     study_report.haml
    }
   }
   entity_list=entity_list{
    Gene=Gene{
     study_report.haml
     incidence.haml
    }
    GenomicMutation=GenomicMutation{
     study_report.haml
    }
   }
  }
 }
}
